{
  "name": "creative-render-service",
  "version": "1.0.0",
  "engines": {
    "node": "8.9.4"
  },
  "description": "Renders the creative.",
  "scripts": {
    "start": "npm run build && (run-p start:app start:api)",
    "start:prod": "npm run build:prod && (run-p start:app start:api:prod)",
    "start:build": "run-p start:app start:api:prod",
    "start:app": "node build/server.js",
    "start:api": "nodemon --watch ./api --exec ts-node api/index.ts",
    "start:api:prod": "node build/api",
    "clean": "rimraf build",
    "build": "npm run clean && (run-s build:app build:server)",
    "build:prod": "npm run clean && (run-s build:app:prod build:server:prod build:api)",
    "build:app": "webpack --config config/webpack/app.webpack.ts --display-error-details",
    "build:app:prod": "webpack --config config/webpack/app.webpack.prod.ts --display-error-details",
    "build:server": "webpack --config config/webpack/server.webpack.ts --display-error-details",
    "build:server:prod": "NODE_ENV=production webpack --config config/webpack/server.webpack.ts --display-error-details",
    "build:api": "webpack --config config/webpack/api.webpack.ts --display-error-details",
    "test": "karma start config/test/karma.conf.js",
    "test:ci": "better-npm-run test:ci",
    "test:api": "better-npm-run test:api",
    "lint": "tslint -p tsconfig.json --type-check",
    "deploy": "ts-node ./scripts/deploy.ts $*",
    "build:image": "ENV=production bash scripts/build_image.sh",
    "upload": "ts-node ./scripts/upload.ts",
    "preupload": "run-p build:lib build:lib:prod"
  },
  "author": "Levi Lu <xinghu1989@gmail.com>",
  "homepage": "https://github.com/hmward/creative-render-service.git",
  "repository": {
    "type": "git",
    "url": "git@github.com:hmward/creative-render-service.git"
